{
	"name" : "fgsl/handler",
	"description" : "Fgsl Handlerk",
	"type" : "framework",
	"keywords" : [
		"framework",
		"laminas",
		"mezzio",
		"microservice"
	],
	"homepage" : "https://github.com/fgsl/handler",
	"license" : "AGPL-3.0-only",
	"autoload" : {
		"psr-4" : {
			"Fgsl\\Handler\\" : "src/Fgsl/Handler",
			"Fgsl\\Test\\Handler\\" : "tests/Fgsl/Handler"
		}
	},
	"require" : {
		"php" : ">=7.4",
		"laminas/laminas-authentication" : "^2.7",
		"laminas/laminas-diactoros" : "^1.7.1 || ^2.0",
		"mezzio/mezzio" : "^3.0.1",
		"mezzio/mezzio-helpers" : "^5.0",
		"mezzio/mezzio-laminasrouter" : "^3.0",
		"mezzio/mezzio-laminasviewrenderer" : "^2.0",
		"laminas/laminas-permissions-rbac" : "^3.0",
		"laminas/laminas-servicemanager" : "^3.3",
		"laminas/laminas-session" : "^2.9",
		"fgsl/jwt": "^1.1",
		"fgsl/http": "^1.2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.0"
	},
	"config" : {
		"secure-http" : false
	}
}
